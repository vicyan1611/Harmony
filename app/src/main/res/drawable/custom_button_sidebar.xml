<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <solid android:color="@color/primary_gray_light" />
            <stroke
                android:width="0dp"
                android:color="@color/white" />
            <padding
                android:bottom="5dp"
                android:left="5dp"
                android:right="5dp"
                android:top="5dp" />
            <size
                android:width="52dp"
                android:height="52dp" />
        </shape>
    </item>
    <item
        android:width="32dp"
        android:height="24dp"
        android:drawable="@drawable/discord_symbol_white"
        android:gravity="center" />
</layer-list>